/* pages/component/head.wxss */
.header {
  width: 750rpx;
  height: 50rpx
}

.hdCon {
  display: flex;
  justify-content: space-between;
  align-items: center;
  height: 50rpx;
  background-color: white;
}

.hdCon navigator,
.hdCon .kefu {
  height: 50rpx;
}

.hdCon image {
  height: 100%;
}

.kefu {
  position: relative;
  animation: dh linear 1s infinite alternate;
}

.kefu .btn {
  position: absolute;
  width: 100%;
  height: 100%;
  top: 0;
  left: 0;
  opacity: 0;
}
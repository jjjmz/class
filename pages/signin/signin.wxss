/* pages/signin/signin.wxss */
page{
  width: 100%;
  height: 100%;
}
.pickers {
  padding:1rpx 0 20rpx 0;
  margin: 10rpx 20rpx 40rpx 20rpx;
  border-radius: 20rpx;
  background-color: rgb(223, 221, 221);
}
/* 选择 */
.pic {
  margin: 40rpx;
  text-align: center;
  background-color: greenyellow;
  border-radius: 20rpx;
}
.pic picker {
  display: inline-flex;
}
.course {
  margin: 40rpx;
  text-align: center;
  background-color: rgb(131, 194, 252);
  border-radius: 20rpx;
}
.signup {
  border-radius: 50%;
  line-height: 160rpx;
}
